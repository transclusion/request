{
  "name": "@transclusion/request",
  "version": "1.0.1",
  "description": "For making observable requests in the browser and in Node.js.",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "main": "./dist/cjs/server/index.js",
  "module": "./dist/es/server/index.js",
  "browser": {
    "./dist/cjs/server/index.js": "./dist/cjs/browser/index.js",
    "./dist/es/server/index.js": "./dist/es/browser/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.ts",
    "postinstall": "yarn husky install",
    "postpublish": "pinst --enable",
    "prebuild": "yarn clean",
    "prepublishOnly": "pinst --disable && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nock": "^13.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5",
    "xhr-mock": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  }
}
