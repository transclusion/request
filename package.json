{
  "type": "module",
  "name": "@transclusion/request",
  "version": "1.1.1",
  "description": "For making observable requests in the browser and in Node.js.",
  "author": "Marius Lundg√•rd <studio@mariuslundgard.com>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "types": "./dist/dts/types/src/server/index.d.ts",
  "source": "./src/server/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.js": "./dist/index.browser.js"
  },
  "exports": {
    ".": {
      "types": "./dist/dts/types/src/server/index.d.ts",
      "browser": {
        "require": "./dist/index.browser.cjs",
        "import": "./dist/index.browser.js",
        "default": "./dist/index.browser.js"
      },
      "source": "./src/server/index.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pkg-utils build --tsconfig tsconfig.dist.json",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js,.ts",
    "postpublish": "pinst --enable",
    "prebuild": "yarn clean",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable && yarn build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^1.7.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nock": "^13.2.8",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "semantic-release-cli": "^5.4.4",
    "ts-jest": "^28.0.6",
    "typescript": "^4.7.4",
    "xhr-mock": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/transclusion/request.git"
  }
}
